{
    "name": "yannoff/composer-dotenv-handler",
    "description": "Composer script to generate .env file against .env.dist template",
    "keywords": ["env", "dotenv", "composer"],
    "homepage": "https://github.com/yannoff/composer-dotenv-handler",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannoff",
            "homepage": "https://github.com/yannoff"
        }
    ],
    "require": {
        "php": "^5.5.9|>=7.0.8",
        "symfony/dotenv": ">=3.4"
    },
    "require-dev": {
        "composer/composer": "^1.0@dev",
        "symfony/filesystem": "^2.3 || ^3 || ^4"
    },
    "autoload": {
        "psr-4": { "Yannoff\\DotenvHandler\\": "src/" }
    },
    "extra": {
        "class": "Yannoff\\DotenvHandler\\ScriptHandler"
    }
}
